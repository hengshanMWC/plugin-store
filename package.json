{
  "name": "plugin-group",
  "version": "1.0.3",
  "packageManager": "pnpm@7.21.0",
  "license": "MIT",
  "homepage": "https://github.com/hengshanMWC/plugin-store#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hengshanMWC/plugin-store.git"
  },
  "bugs": {
    "url": "https://github.com/hengshanMWC/plugin-store/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "plugin",
    "store"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "cz",
    "release": "esno ./scripts/release",
    "build": "unbuild",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "lint": "eslint --ext=js,ts,json,md,tsx .",
    "lint:fix": "eslint --fix --ext=js,ts,json,md,tsx .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@abmao/pkgs": "^2.2.1",
    "@antfu/eslint-config-ts": "^0.38.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitest/coverage-c8": "^0.31.0",
    "colors": "^1.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "esno": "^0.16.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vite": "^4.3.4",
    "vitepress": "1.0.0-alpha.29",
    "vitest": "^0.31.0"
  },
  "lint-staged": {
    "{server,client}/*.{js,ts,json,md,tsx}": "pnpm lint"
  },
  "dependencies": {
    "load-code": "^2.1.1"
  }
}
